---
import logo_01 from "@/assets/images/auspiciadores/logo01.png"
import logo_02 from "@/assets/images/auspiciadores/logo02.png"
---

<section class="relative bg-transparent z-10 text-white">
  <div class="container">
    <h3 class="text-center font-amx-bold text-2xl xl:text-[42px]">Auspiciadores</h3>
    <div class="flex justify-center items-center gap-8 pt-6">
      <img class="max-w-[60px] md:max-w-fit h-auto" src={logo_01.src} alt="Bembos">
      <img class="max-w-[80px] md:max-w-fit h-auto" src={logo_02.src} alt="Xiaomi">
    </div>
  </div>
</section>